{
  "name": "@mr-quin/danmaku-anywhere",
  "version": "0.12.0",
  "private": true,
  "description": "Browser extension for danmaku-anywhere",
  "keywords": [],
  "author": "Mr-Quin",
  "repository": "https://github.com/Mr-Quin/danmaku-anywhere",
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:firefox": "TARGET_BROWSER=firefox pnpm run dev",
    "build": "tsc && vite build --outDir build --emptyOutDir",
    "build:firefox": "TARGET_BROWSER=firefox pnpm run build",
    "test": "vitest",
    "preview": "vite preview",
    "lint": "tsc && eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "format": "prettier --write . --ignore-path ../../.prettierignore",
    "package": "node ./scripts/package.js",
    "package:firefox": "TARGET_BROWSER=firefox pnpm run package",
    "tsc": "tsc"
  },
  "dependencies": {
    "@danmaku-anywhere/dandanplay-api": "workspace:*",
    "@danmaku-anywhere/danmaku-engine": "workspace:*",
    "@danmaku-anywhere/danmaku-converter": "workspace:*",
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.13",
    "@hookform/resolvers": "^3.9.0",
    "@mui/icons-material": "^5.16.5",
    "@mui/lab": "5.0.0-alpha.173",
    "@mui/material": "^5.16.5",
    "@tanstack/react-query": "^5.51.15",
    "@tanstack/react-query-devtools": "^5.51.15",
    "@tanstack/react-virtual": "^3.8.3",
    "@use-gesture/react": "^10.3.1",
    "dexie": "^4.0.8",
    "i18next": "^23.12.2",
    "immer": "^10.1.1",
    "pinyin-pro": "^3.23.1",
    "react": "18.3.0-canary-c3048aab4-20240326",
    "react-dom": "18.3.0-canary-c3048aab4-20240326",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.52.1",
    "react-i18next": "^15.0.0",
    "react-router-dom": "^6.25.1",
    "ts-pattern": "^5.2.0",
    "urlpattern-polyfill": "^10.0.0",
    "zod": "^3.23.8",
    "zustand": "5.0.0-alpha.5"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.23",
    "@types/chrome": "^0.0.269",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@vitejs/plugin-react": "^4.3.1",
    "vite": "^5.3.5",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^2.0.4"
  }
}
