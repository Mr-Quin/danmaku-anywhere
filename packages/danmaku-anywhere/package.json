    "@tanstack/react-virtual": "^3.1.2",
    "dexie": "^3.2.5",
    "dexie-react-hooks": "^1.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.18",
    "@types/chrome": "^0.0.242",
