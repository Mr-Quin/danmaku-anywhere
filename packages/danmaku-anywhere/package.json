{
  "name": "@mr-quin/danmaku-anywhere",
  "version": "0.15.3",
  "private": true,
  "description": "Browser extension for danmaku-anywhere",
  "keywords": [],
  "author": "Mr-Quin",
  "repository": "https://github.com/Mr-Quin/danmaku-anywhere",
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:firefox": "TARGET_BROWSER=firefox pnpm run dev",
    "build": "tsc && vite build --outDir build --emptyOutDir",
    "build:firefox": "TARGET_BROWSER=firefox pnpm run build",
    "test": "vitest",
    "preview": "vite preview",
    "lint": "tsc && eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "format": "prettier --write . --ignore-path ../../.prettierignore",
    "package": "node ./scripts/package.js",
    "package:firefox": "TARGET_BROWSER=firefox pnpm run package",
    "tsc": "tsc"
  },
  "dependencies": {
    "@danmaku-anywhere/danmaku-converter": "workspace:*",
    "@danmaku-anywhere/danmaku-engine": "workspace:*",
    "@danmaku-anywhere/danmaku-provider": "workspace:*",
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@hookform/resolvers": "^3.9.0",
    "@mui/icons-material": "^6.1.5",
    "@mui/lab": "6.0.0-beta.13",
    "@mui/material": "^6.1.5",
    "@mui/system": "^6.1.5",
    "@tanstack/react-query": "^5.59.16",
    "@tanstack/react-query-devtools": "^5.59.16",
    "@tanstack/react-virtual": "^3.10.8",
    "@use-gesture/react": "^10.3.1",
    "dexie": "^4.0.9",
    "i18next": "^23.16.2",
    "immer": "^10.1.1",
    "pinyin-pro": "^3.25.0",
    "react": "18.3.0-canary-c3048aab4-20240326",
    "react-dom": "18.3.0-canary-c3048aab4-20240326",
    "react-error-boundary": "^4.1.2",
    "react-hook-form": "^7.53.1",
    "react-hotkeys-hook": "^4.5.1",
    "react-i18next": "^15.1.0",
    "react-router-dom": "^6.27.0",
    "ts-pattern": "^5.5.0",
    "urlpattern-polyfill": "^10.0.0",
    "zod": "^3.23.8",
    "zustand": "5.0.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.26",
    "@types/chrome": "^0.0.279",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@vitejs/plugin-react": "^4.3.3",
    "vite": "^5.4.10",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^2.1.3"
  }
}
