{
  "name": "danmaku-anywhere-proxy",
  "main": "src/index.ts",
  "compatibility_date": "2025-05-05",
  "compatibility_flags": ["nodejs_compat"],
  "secrets_store_secrets": [
    {
      "binding": "DANDANPLAY_APP_SECRET",
      "store_id": "4a9ca50edba84431879f34b0b67f9998",
      "secret_name": "DANDANPLAY_APP_SECRET"
    },
    {
      "binding": "DANMAKU_GEMINI_API_KEY",
      "store_id": "4a9ca50edba84431879f34b0b67f9998",
      "secret_name": "DANMAKU_GEMINI_API_KEY"
    },
    {
      "binding": "DA_AI_GATEWAY_ID",
      "store_id": "4a9ca50edba84431879f34b0b67f9998",
      "secret_name": "DA_AI_GATEWAY_ID"
    },
    {
      "binding": "DA_AI_GATEWAY_NAME",
      "store_id": "4a9ca50edba84431879f34b0b67f9998",
      "secret_name": "DA_AI_GATEWAY_NAME"
    }
  ],
  "version_metadata": {
    "binding": "CF_VERSION_METADATA"
  },
  "vars": {
    "ENVIRONMENT": "dev",
    "DANDANPLAY_API_HOST": "https://api.dandanplay.net",
    "ALLOWED_ORIGIN": "https://danmaku.weeblify.app",
    "DANDANPLAY_APP_ID": "64fw9qcpfb",
    "GEMINI_MODEL": "gemini-2.5-flash"
  },
  "observability": {
    "enabled": true
  },
  "route": "danmaku.weeblify.app/proxy/*",
  "env": {
    "staging": {
      "vars": {
        "ENVIRONMENT": "staging",
        "DANDANPLAY_API_HOST": "https://api.dandanplay.net",
        "ALLOWED_ORIGIN": "https://danmaku.weeblify.app,https://danmaku-staging.weeblify.app,http://localhost:4200",
        "DANDANPLAY_APP_ID": "64fw9qcpfb",
        "GEMINI_MODEL": "gemini-2.5-flash"
      },
      "version_metadata": {
        "binding": "CF_VERSION_METADATA"
      },
      "secrets_store_secrets": [
        {
          "binding": "DANDANPLAY_APP_SECRET",
          "store_id": "4a9ca50edba84431879f34b0b67f9998",
          "secret_name": "DANDANPLAY_APP_SECRET"
        },
        {
          "binding": "DANMAKU_GEMINI_API_KEY",
          "store_id": "4a9ca50edba84431879f34b0b67f9998",
          "secret_name": "DANMAKU_GEMINI_API_KEY_STG"
        },
        {
          "binding": "DA_AI_GATEWAY_ID",
          "store_id": "4a9ca50edba84431879f34b0b67f9998",
          "secret_name": "DA_AI_GATEWAY_ID_STG"
        },
        {
          "binding": "DA_AI_GATEWAY_NAME",
          "store_id": "4a9ca50edba84431879f34b0b67f9998",
          "secret_name": "DA_AI_GATEWAY_NAME_STG"
        }
      ],
      "routes": [
        {
          "pattern": "api.danmaku-staging.weeblify.app",
          "custom_domain": true
        }
      ]
    },
    "production": {
      "vars": {
        "ENVIRONMENT": "production",
        "DANDANPLAY_API_HOST": "https://api.dandanplay.net",
        "ALLOWED_ORIGIN": "https://danmaku.weeblify.app",
        "DANDANPLAY_APP_ID": "64fw9qcpfb",
        "GEMINI_MODEL": "gemini-2.5-flash"
      },
      "version_metadata": {
        "binding": "CF_VERSION_METADATA"
      },
      "secrets_store_secrets": [
        {
          "binding": "DANDANPLAY_APP_SECRET",
          "store_id": "4a9ca50edba84431879f34b0b67f9998",
          "secret_name": "DANDANPLAY_APP_SECRET"
        },
        {
          "binding": "DANMAKU_GEMINI_API_KEY",
          "store_id": "4a9ca50edba84431879f34b0b67f9998",
          "secret_name": "DANMAKU_GEMINI_API_KEY"
        },
        {
          "binding": "DA_AI_GATEWAY_ID",
          "store_id": "4a9ca50edba84431879f34b0b67f9998",
          "secret_name": "DA_AI_GATEWAY_ID"
        },
        {
          "binding": "DA_AI_GATEWAY_NAME",
          "store_id": "4a9ca50edba84431879f34b0b67f9998",
          "secret_name": "DA_AI_GATEWAY_NAME"
        }
      ],
      "routes": [
        {
          "pattern": "api.danmaku.weeblify.app",
          "custom_domain": true
        }
      ]
    }
  }
}
